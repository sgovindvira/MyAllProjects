package Day3SDETQA;

public class VowerAndConso {

	public static void main(String[] args) {
	
		String sanjay = "Sanajy";
		
		
		if(sanjay.contains("a")) {
			System.out.println("It contains Vowel");
		}else {System.out.println("NNNNNNNNNNNNNNNNNNN");
		}
		
		//ORRRRRRRRRRRRRRRRRRRRRRRRRR///
		
		char ch = 'a';
		if(ch =='a'||ch=='e'|| ch=='o'||ch=='u'||ch=='i')
		
		{System.out.println(ch + "is vowel");
		
		}else {
			System.out.println(ch +" " + " is consonant");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
}
}