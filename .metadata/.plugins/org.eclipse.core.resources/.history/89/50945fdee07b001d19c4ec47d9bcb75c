package Day3SDETQA;

public class WhileLoop {

	public static void main(String[] args) {
	int i =1;
		
	/*	while (i<=10) {
			System.out.println(i);
			i++;
		}*/
	
	
	
	
	
	
	
	
	// How to print even number between 1-10
	i=2;// change to 1 for odd number
	
	while(i<=10) {
		System.out.println(i);
		i= i+2;
	}
	
	
	// how to print decreament numbers
	
	
	
	int j =10;
	while(j>1) {
		System.out.println(j);
		i--;
	}
	
	
	
	
	
		
	
	}

}
